SL=iris$Sepal.Length
SW=iris$Sepal.Width
PL=iris$Petal.Length
PW=iris$Petal.Width
par(mfrow=c(2,2))
My=round(mean(SL),3)
cat("My=",My)
Va=round(var(SL),3)
cat("Var=",Va)
Mn=min(SL)
cat("Mn=",Mn)
Mx=max(SL)
cat("Mx=",Mx)
plot(ecdf(SL),main="Courbe cumulative de SL",col="red",xlab="Longueur",ylab="Fréquences cumulées")
x=seq(Mn-1,Mx+1,0.1)
y=pnorm(x,My,sqrt(Va))
lines(x,y,type="l",col="blue",ylim=c(0,max(y)+0.2))
legend(4,0.98,legend=paste(c("Moy=","Var="),c(My,Va),sep=""),box.col="green",cex=0.7)
legend(6.5,0.2,legend=c("Norm.Cumu","SL.Cumul"),col=c("blue","red"),lty=1:2, cex=0.7,box.col="purple")
My1=round(mean(SW),3)
cat("My=",My1)
Va1=round(var(SW),3)
cat("Var=",Va1)
Mn1=min(SW)
cat("Mn=",Mn1)
Mx1=max(SW)
cat("Mx=",Mx1)
plot(ecdf(SW),main="Courbe cumulative de SW",col="red",xlab="Largeur",ylab="Fréquences cumulées")
x1=seq(Mn1-1,Mx1+1,0.1)
y1=pnorm(x1,My1,sqrt(Va1))
lines(x1,y1,type="l",col="blue",ylim=c(0,max(y1)+0.2))
legend(Mn1,0.98,legend=paste(c("Moy=","Var="),c(My1,Va1),sep=""),box.col="green",cex=0.7)
legend(Mx1-0.5,0.18,legend=c("Norm.Cumu","SW.Cumul"),col=c("blue","red"),lty=1:2, cex=0.7,box.col="purple")
